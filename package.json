{
  "author": "RedKubes",
  "bugs": {
    "url": "https://github.com/redkubes/otomi-tasks/issues"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "@apidevtools/json-schema-ref-parser": "9.0.9",
    "@kubernetes/client-node": "0.16.3",
    "@redkubes/gitea-client-node": "1.1.1",
    "@redkubes/harbor-client-node": "2.2.1",
    "@redkubes/keycloak-client-node": "15.0.0",
    "async-retry": "1.3.3",
    "aws-sdk": "2.1116.0",
    "axios": "0.26.1",
    "bluebird": "3.7.2",
    "cookie": "0.5.0",
    "drone-node": "2.2.0",
    "envalid": "6.0.2",
    "js-yaml": "3.14.1",
    "lodash": "4.17.21",
    "lowdb": "1.0.0",
    "morgan": "1.10.0",
    "node-fetch": "2.6.7",
    "openid-client": "5.1.5",
    "ts-custom-error": "3.2.0"
  },
  "description": "Tasks needed by the Otomi Container Platform to glue all the pieces together.",
  "devDependencies": {
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@hkdobrev/run-if-changed": "0.3.1",
    "@types/async-retry": "1.4.3",
    "@types/chai": "4.3.1",
    "@types/chai-as-promised": "7.1.5",
    "@types/express": "4.17.13",
    "@types/lodash": "4.14.181",
    "@types/lowdb": "1.0.11",
    "@types/mocha": "9.1.0",
    "@types/node": "16.11.27",
    "@types/node-fetch": "2.6.1",
    "@types/sinon": "9.0.11",
    "@types/sinon-chai": "3.2.8",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "chai": "4.3.6",
    "chai-as-promised": "7.1.1",
    "commitizen": "4.2.4",
    "copyfiles": "2.4.1",
    "cz-conventional-changelog": "3.3.0",
    "dotenv": "8.6.0",
    "eslint": "7.32.0",
    "eslint-config-airbnb-base": "14.2.1",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-chai-friendly": "0.7.2",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "3.4.1",
    "git-cz": "4.8.0",
    "github-release-from-changelog": "2.1.1",
    "husky": "7.0.4",
    "jsonwebtoken": "8.5.1",
    "lint-staged": "10.5.4",
    "mocha": "9.2.2",
    "nock": "13.2.4",
    "node-notifier": "8.0.2",
    "npm-run-all": "4.1.5",
    "openapi-schema-validator": "3.0.3",
    "prettier": "2.6.2",
    "sinon": "11.1.2",
    "sinon-chai": "3.7.0",
    "standard-version": "9.3.2",
    "supertest": "4.0.2",
    "ts-node": "10.7.0",
    "ts-node-dev": "1.1.8",
    "typescript": "4.6.3"
  },
  "engines": {
    "node": ">=15.0.0",
    "npm": ">=6.0.0"
  },
  "homepage": "https://github.com/redkubes/otomi-tasks#readme",
  "license": "Apache-2.0",
  "lint-staged": {
    "*.{json,md,yml,yaml}": [
      "prettier --write"
    ],
    "*.ts": [
      "eslint --fix"
    ]
  },
  "main": "",
  "name": "otomi-tasks",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/redkubes/otomi-tasks.git"
  },
  "run-if-changed": {
    "package-lock.json": "npm install --prefer-offline --no-audit"
  },
  "scripts": {
    "build": "tsc",
    "cz:retry": "git-cz --retry",
    "cz": "git-cz",
    "lint-staged": "lint-staged",
    "lint:es:fix": "eslint --fix --ext ts .",
    "lint:es": "eslint --ext ts .",
    "lint:types": "tsc",
    "lint": "run-p lint:types lint:es",
    "prepare": "husky install",
    "release": "standard-version",
    "run-if-changed": "run-if-changed",
    "tasks:certs-aws-dev": "ts-node-dev ./src/tasks/otomi/certs-aws.ts",
    "tasks:certs-aws": "node dist/tasks/otomi/certs-aws.js",
    "tasks:copy-certs-dev": "ts-node-dev ./src/tasks/otomi/copy-certs.ts",
    "tasks:copy-certs": "node dist/tasks/otomi/copy-certs.js",
    "tasks:drone-dev": "ts-node-dev ./src/tasks/drone/drone.ts",
    "tasks:drone": "node dist/tasks/drone/drone.js",
    "tasks:gitea-add-users-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/gitea/gitea-add-users-to-team.ts",
    "tasks:gitea-add-users": "node dist/tasks/gitea/gitea-add-users-to-team.js",
    "tasks:gitea-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/gitea/gitea.ts && ts-node-dev ./src/tasks/gitea/gitea-drone-oauth.ts",
    "tasks:gitea-drone-auth-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/gitea/gitea-drone-oauth.ts",
    "tasks:gitea-drone-auth": "node dist/tasks/gitea/gitea-drone-oauth.js",
    "tasks:gitea": "node dist/tasks/gitea/gitea.js && node dist/tasks/gitea/gitea-drone-oauth.js",
    "tasks:harbor-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/harbor/harbor.ts",
    "tasks:harbor": "node dist/tasks/harbor/harbor.js",
    "tasks:keycloak-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/keycloak/keycloak.ts",
    "tasks:keycloak": "node dist/tasks/keycloak/keycloak.js",
    "tasks:otomi-chart-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/otomi/otomi-chart.ts",
    "tasks:otomi-chart": "node dist/tasks/otomi/otomi-chart.js",
    "tasks:wait-for-dev": "NODE_TLS_REJECT_UNAUTHORIZED=0 ts-node-dev ./src/tasks/otomi/wait-for.ts",
    "tasks:wait-for": "node dist/tasks/otomi/wait-for.js",
    "test": "NODE_ENV=test mocha -r ts-node/register -r ts-custom-error --exit src/**/*.test.*"
  },
  "standard-version": {
    "skip": {
      "tag": true
    }
  },
  "version": "0.2.25"
}
